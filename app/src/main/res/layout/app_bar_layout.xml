<!--                            app:iss_auto_cycle="true"-->
<!--                            app:iss_period="3000"-->
<!--                            app:iss_delay="1000"-->
<!--                            app:iss_no_dots="true"-->
<!--                            app:iss_text_align="CENTER"-->
<!--                            app:layout_constraintStart_toStartOf="parent"-->
<!--                            android:layout_marginEnd="5dp"-->
<!--                            android:layout_marginStart="5dp"-->
<!--                            android:scaleType="fitXY" />-->

                        <!-- Right Banner Image Slider -->
<!--                        <com.denzcoskun.imageslider.ImageSlider-->
<!--                            android:id="@+id/right_banner_image_slider"-->
<!--                            android:layout_width="40dp"-->
<!--                            android:layout_height="match_parent"-->
<!--                            android:clipToPadding="false"-->
<!--                            app:iss_auto_cycle="true"-->
<!--                            app:iss_period="3000"-->
<!--                            app:iss_delay="1000"-->
<!--                            app:iss_no_dots="true"-->
<!--                            app:iss_text_align="CENTER"-->
<!--                            app:layout_constraintEnd_toEndOf="parent"-->
<!--                            android:layout_marginEnd="5dp"-->
<!--                           />-->

                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>





                <RelativeLayout
                    android:id="@+id/quick_link"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    android:layout_marginStart="10dp"
                    android:layout_marginEnd="10dp"
                    android:layout_marginTop="10dp"
                    app:layout_constraintTop_toBottomOf="@+id/cardview_image_slider"
                    >

                   <RelativeLayout
                       android:id="@+id/inner_rl"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"



                       >

                           <androidx.cardview.widget.CardView
                               android:id="@+id/txt_quick_link"
                               android:layout_width="match_parent"
                               android:layout_height="wrap_content"
                               app:layout_constraintEnd_toEndOf="parent"
                               app:layout_constraintStart_toStartOf="parent"
                               android:backgroundTint="@color/blue_theme_color"
                               app:cardCornerRadius="10dp"
                               >
                               <TextView
                                   android:id="@+id/txt_quick_link_txt"
                                   android:layout_width="match_parent"
                                   android:layout_height="wrap_content"
                                   android:text="Quick Links"
                                   android:fontFamily="@font/bodoni_moda"
                                   android:textColor="@color/white"
                                   android:textStyle="bold"
                                   android:textSize="16sp"
                                   app:layout_constraintStart_toStartOf="parent"
                                   app:layout_constraintEnd_toEndOf="parent"
                                   android:layout_marginTop="0dp"
                                   android:padding="5dp"
                                   android:layout_marginStart="10dp"
                                   android:layout_marginBottom="0dp"
                                   tools:ignore="MissingConstraints" />
                           </androidx.cardview.widget.CardView>
                           <LinearLayout
                               android:id="@+id/quick_link_layout"
                               android:layout_below="@+id/txt_quick_link"
                               android:layout_width="match_parent"
                               android:layout_height="wrap_content"
                               android:baselineAligned="false"
                               android:weightSum="4"
                               android:layout_marginBottom="10dp"
                               android:layout_marginTop="0dp"
                               android:orientation="horizontal"
                               android:gravity="center_horizontal"
                               android:padding="0dp"

                               >
                               <LinearLayout
                                   android:id="@+id/bbps_recharge"
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:orientation="vertical"
                                   android:layout_gravity="center"
                                   android:layout_weight="1"
                                   android:layout_margin="0dp"
                                   android:padding="3dp"
                                   android:gravity="center"
                                   tools:ignore="UseCompoundDrawables">
                                   <ImageView
                                       android:id="@+id/bbps_recharge_img"
                                       android:layout_width="wrap_content"
                                       android:layout_height="40dp"
                                       android:layout_marginTop="10dp"
                                       android:contentDescription="@string/todo"
                                       android:src="@drawable/bbps_logo"
                                       />
                                   <TextView
                                       android:id="@+id/bbps_recharge_txt"
                                       android:layout_width="wrap_content"
                                       android:layout_height="wrap_content"
                                       android:layout_marginTop="5dp"
                                       android:maxLines="2"
                                       android:text="BBPS \nRecharge"
                                       android:textStyle="normal"
                                       android:textColor="@color/logo_blue"
                                       android:gravity="center"

                                       />

                               </LinearLayout>
                               <LinearLayout
                                   android:id="@+id/business_registration_ql"
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:orientation="vertical"
                                   android:layout_gravity="center"
                                   android:gravity="center"
                                   android:layout_margin="2dp"
                                   android:padding="3dp"
                                   android:layout_weight="1"
                                   tools:ignore="UseCompoundDrawables">
                                   <ImageView
                                       android:id="@+id/business_registration_ql_img"
                                       android:layout_width="wrap_content"
                                       android:layout_height="40dp"
                                       android:layout_marginTop="10dp"
                                       android:contentDescription="@string/todo"
                                       android:src="@drawable/company_registration"
                                       />
                                   <TextView
                                       android:id="@+id/business_registration_txt"
                                       android:layout_width="wrap_content"
                                       android:layout_height="wrap_content"
                                       android:layout_marginTop="5dp"
                                       android:text="Business \nRegistration"

                                       android:textColor="@color/logo_blue"
                                       android:gravity="center"
                                       />

                               </LinearLayout>
                               <LinearLayout
                                   android:id="@+id/licence_registration_ql"
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:orientation="vertical"
                                   android:layout_gravity="center"
                                   android:gravity="center"
                                   android:layout_margin="2dp"
                                   android:padding="3dp"
                                   android:layout_weight="1"
                                   tools:ignore="UseCompoundDrawables">
                                   <ImageView
                                       android:id="@+id/licence_registration_ql_img"
                                       android:layout_width="wrap_content"
                                       android:layout_height="40dp"
                                       android:layout_marginTop="10dp"
                                       android:contentDescription="@string/todo"
                                       android:src="@drawable/license_registration"
                                       />
                                   <TextView
                                       android:id="@+id/licence_registration_txt"
                                       android:layout_width="wrap_content"
                                       android:layout_height="wrap_content"
                                       android:layout_marginTop="5dp"
                                       android:text="Licence \nRegistration"
                                       android:maxLines="2"
                                       android:textColor="@color/logo_blue"
                                       android:gravity="center"
                                       />

                               </LinearLayout>
                               <LinearLayout
